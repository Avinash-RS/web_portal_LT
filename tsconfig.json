{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "disableTypeScriptVersionCheck": true,
    "enableIvy":false
  },
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "paths": {
      "@admin/*": [
        "src/app/admin/*"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@learner/*": [
        "src/app/learner/*"
      ],
      "@wca/*": [
        "src/app/wca/*"
      ],
      "@pipes/*": [
        "src/app/pipes"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "@env/*": [
        "src/environments/*"
      ]
    },
    "types": [
      "node"
    ],
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom",
      "esnext.asynciterable",
      "ESNext"
    ]
  }
}